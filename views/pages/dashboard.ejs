<%- include('../common/head') %>

    <body>
        <header>
            <%- include('../common/header') %>
        </header>
        <div class="container">

            <div class="firstLineView">
                <button name="isHoliday">האם חג קרוב השבוע</button>
                <button name="season">עונה נוכחית</button>
            </div>
            

            <div class="chart_view">
                <div id="pieChart" style="width: 100%; height: 100%">
                    <%- include('../components/pieChart') %>
                </div>
            </div>
            

            <div class="flexbox-container">

                <div class="flexbox-item flexbox-item-1">
                    <div class="allNetStock">
                        מלאים כללים בכל הרשת
                        <br/>
                    </div>
                    <div class="flexbox-item flexbox-item-2">
                        <div class="card">
                            <div class="card-item-1">
                                <img src="/Lemon.jpg" width="50" alt="lemonLogo">
                                Lemon
                                <div class="quantity">45 KG</div>
                            </div>
                        </div>
                        
                        <div class="card">
                            <div class="card-item-2">
                                <img src="/Chocholate.jpg" width="50" alt="chocholateLogo">
                                Chocolate
                                <div class="quantity">80 KG</div>
                            </div>
                        </div>

                        <div class="card">
                            <div class="card-item-3">
                                <img src="/Vanilla.jpg" width="50" alt="vanillaLogo">
                                Vanilla
                                <div class="qunatity">75 KG</div>
                            </div>
                        </div>

                        <div class="card">
                            <div class="card-item-4">
                                <img src="/Strawberry.jpg" width="50" alt="strawberryLogo">
                                Strawberry
                                <div class="quantity">35 KG</div>
                            </div>
                        </div>

                        <div class="card">
                            <div class="card-item-5">
                                <img src="/Halva.jpg" width="50" alt="halvaLogo">
                                Halva
                                <div class="quantity">45 KG</div>
                            </div>
                        </div>

                    </div>
                </div>
                

            </div>



        </div>
        <footer>
            <!-- <%- include('../common/footer') %> -->
        </footer>
        <style>
            .container{
                position:absolute;
                margin-top: 70px;
                left: 0;
            }
        </style>
    </body>

    </html>